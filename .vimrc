map j gj
map k gk
set tabstop=4
set autoindent
filetype indent on
filetype plugin on
set wildmode=list:longest,full
set wildmenu
set incsearch
set hidden
set splitright
set splitbelow
set shiftround
set nostartofline
set smarttab
set ttyfast
set fileformats=unix,dos
set fileformat=unix
set wildignore+=*.aux,*.log,*.class
" line numbers
set nu
" Highlight search terms
set hlsearch
set shortmess+=I
set nospell
